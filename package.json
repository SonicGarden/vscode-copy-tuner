{
  "name": "copy-tuner",
  "displayName": "CopyTuner",
  "description": "CopyTuner code action provider.",
  "version": "0.3.3",
  "publisher": "aki77",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SonicGarden/vscode-copy-tuner.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SonicGarden/vscode-copy-tuner/issues"
  },
  "private": true,
  "activationEvents": [
    "onLanguage:ruby",
    "onLanguage:haml",
    "onLanguage:erb",
    "onLanguage:slim"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copyTuner.download",
        "category": "CopyTuner",
        "title": "Download copy_tuner.yml"
      },
      {
        "command": "copyTuner.goto",
        "category": "CopyTuner",
        "title": "Go to CopyTuner Site"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.x",
    "@types/vscode": "^1.37.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "vscode-test": "^1.0.2"
  },
  "extensionDependencies": [
    "aki77.rails-i18n"
  ],
  "dependencies": {
    "execa": "^7.1.1"
  },
  "volta": {
    "node": "16.20.0",
    "yarn": "1.22.19"
  }
}
