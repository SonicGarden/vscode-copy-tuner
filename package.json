{
  "name": "copy-tuner",
  "displayName": "CopyTuner",
  "description": "CopyTuner code action provider.",
  "version": "0.3.1",
  "publisher": "aki77",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SonicGarden/vscode-copy-tuner.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SonicGarden/vscode-copy-tuner/issues"
  },
  "private": true,
  "activationEvents": [
    "onLanguage:ruby",
    "onLanguage:haml",
    "onLanguage:erb",
    "onLanguage:slim"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copyTuner.download",
        "category": "CopyTuner",
        "title": "Download copy_tuner.yml"
      },
      {
        "command": "copyTuner.goto",
        "category": "CopyTuner",
        "title": "Go to CopyTuner Site"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14",
    "@types/vscode": "^1.37.0",
    "glob": "^7.1.4",
    "mocha": "^9.1.3",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4",
    "vscode-test": "^1.0.2"
  },
  "extensionDependencies": [
    "aki77.rails-i18n"
  ],
  "dependencies": {
    "execa": "^5.1.1"
  }
}
